# Local Docker Environment Variables
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_database_name

POSTGRES_PORT=15432
BACKEND_PORT=8000

# Database Connection String
# Use the docker service name 'db' for the host when running in Docker
# Format: postgresql+psycopg://user:password@host:port/database
DATABASE_URL=postgresql+psycopg://your_postgres_user:your_postgres_password@db:5432/your_database_name

# Security Configuration
secret_key=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# API Keys
VOYAGE_API_KEY=your_voyage_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
